{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/hello.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-working.js", 
      "use": "@vercel/node"
    },
    {
      "src": "api/test-project.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/hello",
      "dest": "/api/hello.js"
    },
    {
      "src": "/api/test-working", 
      "dest": "/api/test-working.js"
    },
    {
      "src": "/api/test-project",
      "dest": "/api/test-project.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/test-api.html",
      "dest": "/test-api.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}